<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Headline</th>
      <th>Description</th>
      <th>Type</th>
      <th>Phone</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Url</th>
      <th>Logo url</th>
      <th>Summary</th>
      <th>Attractions</th>
      <th>Photo urls</th>
      <th>Opening date</th>
      <th>Number of rooms</th>
      <th>Theme</th>
      <th>Total gaming space</th>
      <th>Permanent space</th>
      <th>Casino type</th>
      <th>Owner</th>
      <th>Renovated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.title %></td>
        <td><%= venue.headline %></td>
        <td><%= venue.description %></td>
        <td><%= venue.type %></td>
        <td><%= venue.phone %></td>
        <td><%= venue.address1 %></td>
        <td><%= venue.address2 %></td>
        <td><%= venue.city %></td>
        <td><%= venue.state %></td>
        <td><%= venue.latitude %></td>
        <td><%= venue.longitude %></td>
        <td><%= venue.url %></td>
        <td><%= venue.logo_url %></td>
        <td><%= venue.summary %></td>
        <td><%= venue.attractions %></td>
        <td><%= venue.photo_urls %></td>
        <td><%= venue.opening_date %></td>
        <td><%= venue.number_of_rooms %></td>
        <td><%= venue.theme %></td>
        <td><%= venue.total_gaming_space %></td>
        <td><%= venue.permanent_space %></td>
        <td><%= venue.casino_type %></td>
        <td><%= venue.owner %></td>
        <td><%= venue.renovated_at %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
